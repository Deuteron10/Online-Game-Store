@model IEnumerable<Online_Game_Store.Models.Game>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Games</h2>

<table class="table table-bordered table-hover"">
    <thead>
        <tr>
            <th scope="col">Games</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var game in Model)
        {
            <tr>
                <td>@game.Name</td>
            </tr>
        }

    </tbody>
</table>
